{
  "name": "bun-compression",
  "version": "0.0.4",
  "description": "Hono Compression plugin for Bun",
  "repository": {
    "type": "git",
    "url": "https://github.com/nischitpra/bun-compression"
  },
  "exports": {
    "bun": "./dist/index.js",
    "node": "./dist/cjs/index.js",
    "require": "./dist/cjs/index.js",
    "import": "./dist/index.js",
    "default": "./dist/cjs/index.js"
  },
  "types": "./src/index.ts",
  "url": "https://github.com/nischitpra/bun-compression",
  "keywords": [
    "hono",
    "compression"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "bun run --hot example/index.ts",
    "build": "rm -rf dist && tsc --project tsconfig.esm.json && tsc --project tsconfig.cjs.json",
    "test": "bun test",
    "release": "npm run build && npm publish --access public",
    "fmt": "prettier --write ."
  },
  "peerDependencies": {
    "hono": ">=3.0.0 <5.0.0"
  },
  "devDependencies": {
    "bun-types": "1.0.14",
    "eslint": "8.54.0",
    "prettier": "3.1.0",
    "typescript": "5.3.2"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2,
    "semi": false,
    "arrowParens": "always"
  }
}
